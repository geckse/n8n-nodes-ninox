{
  "name": "n8n-nodes-ninox",
  "version": "2.1.5",
  "publishConfig": { "registry": "https://registry.npmjs.org/" },
  "description": "This package contains two nodes for n8n to interact with Ninox.",
  "keywords": [
    "n8n-community-node-package",
    "n8n-node-package",
    "n8n-nodes",
    "ninox",
    "nodemation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/geckse/n8n-nodes-ninox.git",
  "author": {
    "name": "geckse",
    "email": "mail@geckse.de",
    "homepage": "https://geckse.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/geckse/n8n-nodes-ninox.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/NinoxApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Ninox/Ninox.node.js",
      "dist/nodes/Ninox/NinoxTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "@types/node": "^20.0.0",
    "eslint": "9.32.0",
    "n8n-workflow": "^1.116.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "overrides": {
    "change-case": "4.1.2"
  }
}
